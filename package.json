{
  "name": "haaveersiinee",
  "version": "0.2.0",
  "description": "simple haversine",
  "keywords": [
    "haversine"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
