    "release": "pnpm run test && pnpm run build && release-it"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/flamrdevs/haaveersiinee.git"
  },
  "author": "flamrdevs",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/flamrdevs/haaveersiinee/issues"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.3",
    "@commitlint/config-conventional": "^17.6.3",
    "@commitlint/prompt-cli": "^17.6.3",
    "@types/node": "^20.1.3",
    "release-it": "^15.10.3",
    "typescript": "^5.0.4",
    "vite": "^4.3.5",
    "vite-plugin-dts": "^2.3.0",
    "vitest": "^0.31.0"
  },
  "dependencies": {
    "hvrsn": "^0.2.0"
  }
